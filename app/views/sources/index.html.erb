<h1 class="title is-3 has-text-white">Listing sources</h1>

<table class="table is-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Comments</th>
      <th>Total Listings</th>
      <th>Listings Mapped</th>
      <th>Listings Unmapped</th>
      <th>% Listings Mapped</th>
    </tr>
  </thead>
  <tbody>
    <% @sources.each do |source| %>
      <tr>
        <td><%= source.name %></td>
        <td><%= source.comments %></td>
        <td><%= source.total_entries %></td>
        <td><%= source.total_mapped_entries %></td>
        <td><%= source.total_unmapped_entries %></td>
        <td><%= source.percent_listings_mapped %></td>
      </td>
    <%end%>
  </tbody>
</table>